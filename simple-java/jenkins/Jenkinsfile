pipeline {
    agent any
    stages {
        stage('Checkout') {
            steps {
                git url: 'https://github.com/Koushik0802/MyRepo.git', branch: 'main'
            }
        }
        stage('Compile') {
            steps {
              javac Javafile.java
            }
        }
        stage('Execute') {
            steps {
                java javafile
            }
        }
    }
}
