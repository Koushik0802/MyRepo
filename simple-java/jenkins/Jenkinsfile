pipeline {
    agent any
    stages {
	stage('SCM') {
            steps {
		git https://github.com/Koushik0802/MyRepo.git   
                echo "Pulling code from GitHub"
            }
        }
        stage('Build') {
            steps {
                sh 'mvn -B -DskipTests clean package'
            }
        }
        stage('Test') {
            steps {
                sh 'mvn test'
            }
        }
    }
}
